    "outDir": "public/dist",
    "target": "es3",
    "module": "es2020",
    "jsx": "react",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": false,
  },
  "exclude": [
    "node_modules"
  ],
}
